                                <button type="button" class="sc-btn sc-btn-secondary sc-btn-with-icon br-4 me-3 p-0 d-flex align-items-center justify-content-center custom-btn-wrapper" aria-label="Back">
                                    <span class="sc-btn-icon sc-btn-icon-chevron"></span>
                                </button>
                                <span class="sidebar-header-text">{{ oct_category.name }}</span>
                            </div>
                            <span class="sc-btn sc-btn-with-icon br-4 no-btn d-flex align-items-center justify-content-center sc-megamenu-close">
                                <span class="sc-btn-icon sc-btn-icon-close"></span>
                            </span>
                        </div>
                        {% if oct_category.view == '1' %}
                            <ul class="list-unstyled">
                                {% for child in oct_category.children %}
                                    <li class="fsz-14 fw-500 d-flex align-items-center justify-content-between">
                                        <a class="flex-grow-1" href="{{ child.href }}">{{ child.name }}</a>
                                        {% if child.children or child.oct_pages %}
                                            <span class="sc-btn sc-btn-with-icon br-4 no-btn d-flex align-items-center justify-content-center sc-btn-children">
                                                <span class="sc-btn-icon sc-btn-icon-chevron"></span>
                                            </span>
