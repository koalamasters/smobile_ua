{% if oct_menu %}
    <div class="sc-megamenu">
        <div class="sidebar-header py-2 ps-4 pe-2 fw-700 dark-text d-flex d-xl-none align-items-center justify-content-between">
            <span class="sidebar-header-text">{{ oct_catalog }}</span>
            <span class="sc-btn sc-btn-with-icon br-4 no-btn d-flex align-items-center justify-content-center sc-megamenu-close">
                <span class="sc-btn-icon sc-btn-icon-close"></span>
            </span>
        </div>
        <nav>
            <ul class="sc-megamenu-list list-unstyled py-xxl-2">
                {% for oct_category in oct_menu %}
                <li class="sc-megamenu-list-item d-flex align-items-center justify-content-between">
                    <a href="{{ oct_category.href }}" {{ oct_category.target }} class="d-flex align-items-center flex-grow-1">
                        {% if oct_category.oct_image %}
                            <img src="{{ oct_category.oct_image }}" alt="{{ oct_category.name }}" width="{{ oct_category.width }}" height="{{ oct_category.height }}" class="me-3">
                        {% endif %}
                        <span class="fsz-14">{{ oct_category.name }}</span>
                    </a>
                    {% if oct_category.children or oct_category.oct_pages %}
                        <span class="sc-btn sc-btn-with-icon br-4 no-btn d-flex align-items-center justify-content-center">
                            <span class="sc-btn-icon sc-btn-icon-chevron"></span>
                        </span>
                    {% endif %}
                    {% if (oct_category.children or oct_category.oct_pages) and oct_category.type == 'standard' %}
