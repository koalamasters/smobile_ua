                    <div class="col-xl-6">
                        <div class="contact-form-wrapper">
                            <div class="contact-form-header">
                                <div class="h2 fw-500">{{ text_question_contact }}</div>
                            </div>
                            <form action="{{ action }}" method="post" enctype="multipart/form-data" class="row contact-form">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label for="inputContactsName" class="fsz-14 dark-text fw-500 pb-1"> {{ entry_name }}</label>
                                        {% if (error_name) or (error_email) or (error_enquiry) or (error_oct_terms) %}
                                            <script>
                                                scNotify('danger', '{% if error_name %}{{ error_name }}{% endif %} {% if error_company %}{{ error_company }}{% endif %}    {% if error_email %}<br />{{ error_email }}{% endif %}{% if error_enquiry %}<br />{{ error_enquiry }}{% endif %}{% if error_oct_terms %}<br />{{ error_oct_terms }}{% endif %}');
                                            </script>
                                        {% endif %}

                                        <input type="text" name="name" value="{{ name }}" id="inputContactsName" class="form-control{% if error_name %} error_style{% endif %}" placeholder="{{ entry_name_placeholder }}" />
                                    </div>
                                </div>

                                <div class="col-md-6 pt-3 pt-md-0 mb-3">
                                    <div class="form-group">
                                        <label for="inputContactsCompany" class="fsz-14 dark-text fw-500 pb-1"> {{ entry_company }}</label>
                                        <input type="company" name="company" value="{{ company }}" id="inputContactsCompany" class="form-control{% if error_company %} error_style{% endif %}" placeholder="{{ entry_company_placeholder }}" />
                                    </div>
                                </div>
                                <div class="col-md-12 pt-3 pt-md-0">
                                    <div class="form-group">
                                        <label for="inputContactsEmail" class="fsz-14 dark-text fw-500 pb-1"> {{ entry_email }}</label>
                                        <input type="email" name="email" value="{{ email }}" id="inputContactsEmail" class="form-control{% if error_email %} error_style{% endif %}" placeholder="{{ entry_email_placeholder }}" />
                                    </div>
                                </div>

                                <div class="col-12">
                                    <div class="form-group py-4">
                                        <label for="inputContactsComment" class="fsz-14 dark-text fw-500 pb-1"> {{ entry_enquiry }}</label>
                                        <textarea name="enquiry" rows="5" id="inputContactsComment" class="form-control{% if error_enquiry %} error_style{% endif %}" rows="7">{{ enquiry }}</textarea>
                                    </div>
                                    {{ captcha }}
                                    {% if text_terms %}
                                        <div class="form-check d-flex pb-4">
                                            <input id="agreeCheaperCheck" type="checkbox" class="form-check-input{% if error_oct_terms %} error_style{% endif %}" name="scales"{% if scales_in is defined and scales_in %} checked{% endif %}>
                                            <label class="form-check-label" for="agreeCheaperCheck">{{ text_terms }}</label>
                                        </div>
                                    {% endif %}
                                    <button class="sc-btn sc-btn-primary br-4 contact-submit" type="submit">{{ button_submit }}</button>
                                </div>
                            </form>
                            <script>
                                $("#inputContactsName, #inputContactsEmail, #inputContactsComment, #agreeCheaperCheck").on("change paste keyup", function() {
                                    $(this).removeClass('error_style');
                                });
                            </script>
                        </div>
                    </div>

                </div>
            </div>
        </div>
        {{ content_bottom }}
    </main>
</div>
{{ footer }}
