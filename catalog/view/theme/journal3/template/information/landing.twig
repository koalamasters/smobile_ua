  function removeActive(_anchorPoints,_clickPoints) {
    _anchorPoints.forEach(anchor_point => {
      anchor_point.classList.remove('active');
    });

    if (_clickPoints.length > 0) {
      _clickPoints[0].classList.add('active');
    }
  }

