    const swiperList = document.querySelectorAll('.swiper[id^="swiper_child"]');
    swiperList.forEach(swiper_item => {
      const swiper_id = '#' + swiper_item.getAttribute('id');
      const swiper_group = swiper_id.substr((swiper_id.length - 2), 1);
      const swiper_index = swiper_id.substr((swiper_id.length - 1), 1);
      const bulletSrcList = document.querySelector(`#color_list${swiper_group}`).innerHTML.trim().split(',');
