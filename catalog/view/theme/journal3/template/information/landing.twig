        }
      },
      on:{
        slideChangeTransitionEnd: function () {
          const index = this.realIndex;
          slideIndex = index + 1;
          let justityAlign;
          switch (index) {
            case 0:
              justityAlign = 'flex-start';
              break;
            case 1:
              justityAlign = 'center';
              break;
            case 2:
              justityAlign = 'flex-end';
              break;
          }
          document.querySelector('#pagination_text').style.justifyContent = justityAlign;
          document.querySelector('#pagination_text > p').innerHTML = paginationTextList[index];

          const slide_product = $(`#swiper_child${slideIndex}1`).find('.swiper-slide.swiper-slide-active').data('product');
