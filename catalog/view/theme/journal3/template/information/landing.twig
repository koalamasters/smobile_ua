          simulateTouch: false,
          roundLengths: true,
          loop: true,
          pagination: {
            el: pagination_id,
            clickable: true,
            renderBullet: function (index, className) {
              let bulletSrc = bulletSrcList[index];
              return `<div class="${className} bulletColor"><div class="bullet-icon mx-auto rounded-full"><img src="${bulletSrc}" class="block w-full h-full object-cover" /></div></div>`;
