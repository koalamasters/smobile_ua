              const index = this.realIndex;
              var swiper_part2 = document.querySelector(`#swiper_child${swiper_group}2`).swiper;
              var swiper_part3 = document.querySelector(`#swiper_child${swiper_group}3`).swiper;
              if (swiper_part2 && swiper_part3) {
                swiper_part2.slideToLoop(index, 800, false);
                swiper_part3.slideToLoop(index, 900, false);
